    "zustand": "4.5.2"
  },
  "jest": {
    "moduleNameMapper": {
      "@blog/(.*)$": "<rootDir>/$1"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      ".+\\.(t|j)s$": [
        "ts-jest",
        {
          "tsconfig": "<rootDir>/tsconfig.build.json",
          "diagnostics": false
        }
      ]
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "./server/**/*.{js,ts}",
      "!./server/main.ts",
      "!./server/app.module.ts",
      "!./server/configs/index.config.ts",
      "!./server/utils/**",
      "!./server/filters/all-exceptions.filter.ts",
      "!./server/middlewares/rate-limit.middleware.ts",
      "!./server/middlewares/user-agent.middleware.ts",
      "!./server/database/database.module.ts",
      "!./server/modules/dynamic-config/**",
      "!./server/modules/email/**",
      "!./server/modules/tasks/**",
      "!./server/mongoose/**",
      "!./server/models/**"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/default-timeout.ts"
    ]
  }
}
