    "redux-thunk": "^2.3.0",
    "rimraf": "^3.0.2",
    "supertest": "^4.0.2",
    "ts-jest": "25.2.0",
    "ts-node": "8.6.2",
    "tsconfig-paths": "3.9.0",
    "tslint": "6.0.0",
    "tslint-react": "^4.2.0",
    "typescript": "3.7.5",
    "url-loader": "^3.0.0",
    "uuid": "^3.4.0",
    "wait-on": "^4.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "./server/**/*.{js,ts}",
      "!./server/main.ts",
      "!./server/app.module.ts",
      "!./server/configs/index.config.ts",
      "!./server/utils/**",
      "!./server/pipes/joi.validation.pipe.ts",
      "!./server/filters/all-exceptions.filter.ts",
      "!./server/middlewares/rate-limit.middleware.ts",
      "!./server/database/database.module.ts",
      "!./server/modules/write.day.reading.module.ts"
    ],
    "globalSetup": "<rootDir>/test/setup-globals.ts",
    "setupFiles": [
      "<rootDir>/test/setup-files.ts"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/default-timeout.ts"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "<rootDir>/tsconfig.build.json",
        "diagnostics": false
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,scss,html}": [
      "prettier --config ./.prettierrc --write",
      "git add"
    ],
    "yarn.lock": [
      "git rm --cached"
    ]
  }
}
