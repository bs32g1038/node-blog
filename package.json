{
  "name": "node-blog",
  "version": "3.8.0",
  "description": "A simple and whole blog",
  "homepage": "https://www.lizc.net",
  "license": "MIT",
  "author": {
    "name": "Jason Li",
    "email": "bs32g1038@163.com"
  },
  "scripts": {
    "init:dev:data": "tsc -p tsconfig.build.json && cross-env NODE_ENV=development node dist/scripts/init_faker_data.js",
    "dev": "concurrently \"npm run dev:server\" \" npm run dev:web \"",
    "dev:server": "cross-env NODE_ENV=development concurrently --handle-input \"wait-on dist/server/main.js && nodemon\" \"tsc -w -p tsconfig.build.json\" ",
    "dev:web": "cross-env NODE_ENV=development node client/dev.server.js",
    "build": "concurrently \"npm run build:server\" \" npm run build:web \"",
    "build:server": "rimraf dist && tsc -p tsconfig.build.json",
    "build:web": "next build",
    "start:prod": "cross-env NODE_ENV=production node dist/server/main.js",
    "lint": "eslint **/*.{ts,js,tsx,jsx} --ignore-path .eslintignore",
    "format": "prettier **/**/*.{ts,tsx,js,jsx} --config ./.prettierrc --ignore-path ./.prettierignore --write",
    "start:debug": "nodemon --config nodemon-debug.json",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:cov": "cross-env NODE_ENV=test jest --coverage --forceExit",
    "test:debug": "cross-env NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.0",
    "@nestjs/common": "^6.11.6",
    "@nestjs/core": "^6.11.6",
    "@nestjs/platform-express": "^6.11.6",
    "@types/faker": "^4.1.9",
    "axios": "^0.19.2",
    "cheerio": "^1.0.0-rc.3",
    "cross-env": "^7.0.0",
    "crypto-js": "^4.0.0",
    "data2xml": "^1.3.4",
    "express": "^4.17.1",
    "express-rate-limit": "^5.0.0",
    "faker": "^4.1.0",
    "helmet": "^3.21.2",
    "highlight.js": "^9.18.1",
    "husky": "^4.2.3",
    "jsonwebtoken": "^8.5.1",
    "log4js": "^6.1.2",
    "lru-cache": "^5.1.1",
    "markdown-it": "^10.0.0",
    "markdown-it-link-attributes": "^3.0.0",
    "mongoose": "^5.8.12",
    "next": "9.2.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.4",
    "scroll-into-view-if-needed": "^2.2.24",
    "validator": "^12.2.0",
    "xss": "^1.0.6"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.0.0-alpha.19",
    "@chakra-ui/core": "^0.5.2",
    "@emotion/core": "^10.0.27",
    "@emotion/styled": "^10.0.27",
    "@nestjs/testing": "^6.11.6",
    "@types/cheerio": "^0.22.16",
    "@types/crypto-js": "^3.1.43",
    "@types/express": "^4.17.2",
    "@types/hapi__joi": "^16.0.9",
    "@types/helmet": "^0.0.45",
    "@types/highlight.js": "^9.12.3",
    "@types/jest": "^25.1.2",
    "@types/jsonwebtoken": "^8.3.7",
    "@types/lodash": "^4.14.149",
    "@types/lru-cache": "^5.1.0",
    "@types/markdown-it": "^0.0.9",
    "@types/mongoose": "^5.7.1",
    "@types/multer": "^1.4.2",
