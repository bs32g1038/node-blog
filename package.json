    "@types/react-dom": "^18.0.10",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.7.10",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "antd": "^5.1.5",
    "axios": "^1.2.3",
    "boring-avatars": "^1.7.0",
    "clipboard": "^2.0.11",
    "clsx": "^1.2.1",
    "dayjs": "^1.11.7",
    "eslint": "^8.32.0",
    "eslint-config-next": "^13.1.5",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "find-root": "^1.1.0",
    "http-proxy-middleware": "^2.0.6",
    "husky": "^8.0.3",
    "jest": "^29.4.0",
    "lint-staged": "^13.1.0",
    "lodash": "^4.17.21",
    "nanoid": "^4.0.0",
    "next": "^13.1.2",
    "next-redux-wrapper": "^8.1.0",
    "prettier": "^2.8.3",
    "react": "18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "rimraf": "^4.1.1",
    "sass": "^1.57.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.4"
  },
  "jest": {
    "moduleNameMapper": {
      "^@blog/(.*)$": "<rootDir>/$1"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": ["ts-jest", {
        "tsconfig": "<rootDir>/tsconfig.build.json",
        "diagnostics": false
      }]
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "./server/**/*.{js,ts}",
      "!./server/main.ts",
      "!./server/app.module.ts",
      "!./server/configs/index.config.ts",
      "!./server/utils/**",
      "!./server/pipes/joi.validation.pipe.ts",
      "!./server/filters/all-exceptions.filter.ts",
      "!./server/middlewares/rate-limit.middleware.ts",
      "!./server/database/database.module.ts",
      "!./server/modules/dynamic-config/**",
      "!./server/modules/email/**",
      "!./server/modules/tasks/**",
      "!./server/joi/schemas/**",
      "!./server/mongoose/**",
      "!./server/models/**"
    ],
    "globalSetup": "<rootDir>/test/setup-globals.ts",
    "setupFilesAfterEnv": [
      "<rootDir>/test/default-timeout.ts"
    ]
  },
  "resolutions": {
    "@types/react": "^17.0.38"
  }
}
