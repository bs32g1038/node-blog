    "@types/validator": "^12.0.1",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "@zeit/next-css": "^1.0.1",
    "antd": "4.0.0-rc.4",
    "clipboard": "^2.0.4",
    "codemirror": "^5.51.0",
    "concurrently": "^5.1.0",
    "dayjs": "^1.8.20",
    "emotion": "^10.0.27",
    "emotion-server": "^10.0.27",
